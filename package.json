    "electron-squirrel-startup": "^1.0.0",
    "framer-motion": "^10.12.17",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.13.0",
    "recharts": "^2.7.1",
    "redux-saga": "^1.2.3",
