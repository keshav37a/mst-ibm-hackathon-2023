  "main": "public/electron.js",
  "scripts": {
    "build": "vite build",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build-deploy": "vite build && firebase deploy",
    "start-client": "vite",
    "start": "vite build && (cd server && yarn start)",
    "dev-desktop": "vite build && (cd server && yarn start) && yarn run electron:dev",
    "electron:dev": "electron-forge start",
    "package": "electron-forge package",
    "make-mac": "yarn build && electron-forge make --platform darwin",
    "make-linux": "yarn build && electron-forge make --platform linux",
    "make": "yarn build && electron-forge make"
  },
  "dependencies": {
    "@carbon/react": "^1.31.3",
